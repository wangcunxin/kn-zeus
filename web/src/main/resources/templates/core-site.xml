<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- Put site-specific property overrides in this file. -->
<!-- hc -->
<configuration>

	<property>
	  <name>fs.defaultFS</name>
	  <value>hdfs://hadoop-hd2</value>
	</property>

	<property>
        <name>hadoop.tmp.dir</name>
        <value>/tmp</value>
    </property>

	<property>
        <name>fs.trash.interval</name>
        <value>420</value>
    </property>

	<property>
	   <name>ha.zookeeper.quorum</name>
	   <value>hadoop-hd1:2181,hadoop-hd2:2181,hadoop-hd3:2181,hadoop-hd4:2181,hadoop-hd5:2181</value>
	 </property>
	
</configuration>